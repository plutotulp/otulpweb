name: otulpweb-server
version: 0.1.0.0
synopsis: www.otulp.net website server
category: Web
build-type: Simple
cabal-version: >= 1.10

executable server
  main-is: server.hs
  hs-source-dirs: src
  ghc-options: -Wall -threaded
  default-language: Haskell2010
  build-depends: base
               , servant
               , servant-server
               , warp
               , wai
               , wai-app-static
